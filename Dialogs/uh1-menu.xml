<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <default>
    <menu n="10">
      <label>Huey Configuration</label>
      <enabled type="bool">true</enabled>
     
	 <item n="1">   
        <label>Select Livery</label>
        <binding>
          <command>nasal</command>
          <script>aircraft.livery.dialog.toggle()</script>
        </binding>
      </item>
	  
	   <item n="2">
        <label>Basic Aircraft</label>
        <binding>
          <command>nasal</command>
          <script>load.clean()</script>
        </binding>
      </item>
	  
	   <item n="3">
        <label>Crop Sprayer ( UH-1, 212 + 412 )</label>
        <binding>
          <command>nasal</command>
          <script>load.spray()</script>
        </binding>
      </item>
	  
	 <item n="4">
        <label>Passenger transport ( 212 + 412, NOT UH-1 )</label>
        <binding>
          <command>nasal</command>
          <script>load.luggage()</script>
        </binding>
      </item>
	  
	   <item n="5">
        <label>SAR / Medivac ( 212 + 412, NOT UH-1 )</label>
        <binding>
          <command>nasal</command>
          <script>load.sar()</script>
        </binding>
      </item>
	  
	   <item n="6">
        <label>Combat ( UH-1, 212 + 412 )</label>
        <binding>
          <command>nasal</command>
          <script>load.guns()</script>
        </binding>
      </item>
     
	  <item n="7">
        <label>Reload weapons</label>
        <binding>
          <command>nasal</command>
          <script>guns.reload_Cannon()</script>
        </binding>
      </item>
	 
    </menu>
  </default>

</PropertyList>
